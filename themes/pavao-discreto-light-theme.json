        "terminal.background": "#DDE8F4",
        "terminal.foreground": "#2D3845",
        "terminalCursor.foreground": "#6D94A6",
        "terminal.border": "#C8D5DE",
        "terminal.selectionBackground": "#C3D0D880",
        "terminal.ansiBlack": "#2D3845",
        "terminal.ansiRed": "#C5455D",
        "terminal.ansiGreen": "#2E7550",
        "terminal.ansiYellow": "#A07030",
        "terminal.ansiBlue": "#3A6A93",
        "terminal.ansiMagenta": "#8D5A9F",
        "terminal.ansiCyan": "#3D8B8B",
        "terminal.ansiWhite": "#B8C2CE",
        "terminal.ansiBrightBlack": "#5D6978",
        "terminal.ansiBrightRed": "#D95668",
        "terminal.ansiBrightGreen": "#3E8660",
        "terminal.ansiBrightYellow": "#B58545",
        "terminal.ansiBrightBlue": "#4A7AA8",
        "terminal.ansiBrightMagenta": "#A06CB5",
        "terminal.ansiBrightCyan": "#4D9C9C",
        "terminal.ansiBrightWhite": "#D0D8E0",
        
